<%# 
PARTIAL ROUTE: dialogs/codeDialog.ejs
PARTIAL DESCRIPTION: Dialog to use when generating Code
LOCAL VARIABLES:
----------------------------------
- locals.desktop (boolean): Is the user's device desktop?
%>
<div style="display: none;" hidden class="dialog-wrapper" id="code-markup-dialog"  data-close-backdrop data-esc>
  <div class="dialog large" data-click-capture>
    <div class="dialog-header">
      <p class="grow-1 flex-row h3 bold">Generate Code Markup</p>
      <%if(locals.desktop===true){%>
        <kbd class="inline" style="margin-right: 4px;"  data-pelem="#close-dialog-esc-code" aria-describedby="close-dialog-esc-code" data-mouse data-popover>ESC</kbd>
        <div id="close-dialog-esc-code" role="tooltip" class="tooltip body2" data-placement="bottom-end">
          Press the Escape key to Close the Dialog
        </div>
      <%}%>
      <button class="icon medium close-dialog" type="button" aria-label="Close Dialog">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Close"><path d="M19 6.41 17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"></path></svg>
      </button>
    </div>
    <div class="dialog-body">
      <form data-highlight-js-form id="code-di-form">
        <div tabindex="-1" class="input-group block mt-1" data-hover="false" data-focus="false" data-error="false" data-blurred="false">
          <label for="code-markup-dialog-input">Code Input:</label>
          <div class="mt-1 text-input block medium disabled" tabindex="-1">
              <textarea name="code-markup-dialog-input" id="code-markup-dialog-input" placeholder="#include <stdio.h>
int main(){
printf(&quot;Hello, World!&quot;);
return 0;
}" rows="6" minlength="1" maxlength="5000" spellcheck="false" autocomplete="off" autocapitalize="off" style="font-family: monospace!important;"></textarea>
          </div>
          <p class="char-counter t-disabled" data-input="code-markup-dialog-input" tabindex="-1">
            <span class="min-counter body2" data-val="1" aria-hidden="false">Character Required</span>
            <span class="max-counter body2" data-val="1000" aria-hidden="true">Characters Remaining</span>
          </p>
        </div>
        <div class="flex-row align-start justify-start gap-3" tabindex="-1">
          <fieldset class="grow-1">
            <legend class="body1 bold">Display Style:</legend>
            <label class="radio">
              <input checked type="radio" id="block-code-dialog" name="code-display-style" value="block-code-dialog" class="primary">
              Block
            </label>
            <label class="radio mt-3">
              <input type="radio" id="inline-code-dialog" name="code-display-style" value="inline-code-dialog" class="primary">
              Inline
            </label>
          </fieldset>
          <div 
          class="input-group grow-1" 
          data-hover="false"
          data-focus="false"
          data-error="false"
          data-blurred="false"
          >
          <div class="flex-column w-100 align-start justify-start grow-1">
            <div class="body1 bold" id="high-theme-label-1">Coding Language:</div>
            <button type="button" role="combobox" aria-haspopup="listbox" aria-controls="highligh-js-theme-popover-1" aria-labelledby="high-theme-label-1" class="select" data-popover="" data-pelem="#highligh-js-theme-popover-1" data-click="" aria-expanded="false" data-force-close="">
              <span style="min-width: 125px;" class="body1">auto</span>
              <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="ArrowDropDown"><path d="m7 10 5 5 5-5z"></path></svg>
            </button>
            <div tabindex="0" id="highligh-js-theme-popover-1" role="listbox" class="select-menu o-xs" data-placement="bottom" style="max-height: 200px;">
