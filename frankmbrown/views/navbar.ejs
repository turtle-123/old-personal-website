<%# 
PARTIAL ROUTE: partials/navbar.ejs
PARTIAL DESCRIPTION: Description...
LOCAL VARIABLES:
----------------------------------
- locals.phone
- locals.development
- locals.desktop
%>
<header role="banner" class="navbar">
  <div class="navbar-1">
    <%if(locals.phone===true){%>
      <button aria-label="Navigate Application" aria-haspopup="true" type="button" class="icon medium" data-drawer-button data-drawer-el="#main-navigation">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Menu"><path d="M3 18h18v-2H3v2zm0-5h18v-2H3v2zm0-7v2h18V6H3z"></path></svg>
      </button>
    <%}else{%>
      <button id="navigation-button" hidden aria-label="Navigate Application" aria-haspopup="true" type="button" class="icon medium" data-drawer-button data-drawer-el="#main-navigation">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Menu"><path d="M3 18h18v-2H3v2zm0-5h18v-2H3v2zm0-7v2h18V6H3z"></path></svg>
      </button>
      <svg class="mr-1 nav-logo" xmlns="http://www.w3.org/2000/svg" width="45px" height="45px" viewBox="0 0 24 24" stroke-width="1.28" stroke="rgba(255, 255, 255, 1)" fill="none" stroke-linecap="round" stroke-linejoin="round">
        <rect x="0" y="0" width="100%" height="100%" stroke="none" stroke-width="0"  rx="5" ry="5"/><g transform="translate(2.40, 2.40) scale(0.8)"><path stroke="none" d="M0 0h24v24H0z" fill="none"/><rect x="3" y="3" width="6" height="6" rx="1"/><rect x="15" y="15" width="6" height="6" rx="1"/>
        <path d="M21 11v-3a2 2 0 0 0 -2 -2h-6l3 3m0 -6l-3 3"/><path d="M3 13v3a2 2 0 0 0 2 2h6l-3 -3m0 6l3 -3"/></g>
      </svg>
    <%}%>
  </div>
  <div class="navbar-2 ">
    <span class="grow-1">
      <span class="navbar-title">
        <%if(locals.phone===false){%>
          frankmbrown.net
        <%}else{%>
          fmb.net
        <%}%>
      </span>
    </span>
    <%if(locals.development===true){%>
      <a href="/test" hx-get="/test" hx-target="#PAGE" hx-indicator="#page-transition-progress" class="button medium icon" hx-push-url="true" style="margin-right: 8px;"><svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Store"><path d="M20 4H4v2h16V4zm1 10v-2l-1-5H4l-1 5v2h1v6h10v-6h4v6h2v-6h1zm-9 4H6v-4h6v4z"></path></svg></a>
    <%}%>
    <%if(locals.desktop===true){%>
      <button style="position: relative; top: 1px;" aria-haspopup="dialog" data-popover data-pelem="#download-spa-menu" data-force-close data-click hidden type="button" class="icon medium" id="download-spa" aria-label="Download Single Page Application">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Download"><path d="M5 20h14v-2H5v2zM19 9h-4V3H9v6H5l7 7 7-7z"></path></svg>
      </button>
    <%}else{%>
      <button id="download-spa" style="position: relative; top: 1px;" hidden type="button" class="icon medium" hx-get="/download-spa-mobile" href="/download-spa-mobile" hx-push-url="true" hx-target="#PAGE" hx-indicator="#page-transition-progress" aria-label="Download Single Page Application">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Download"><path d="M5 20h14v-2H5v2zM19 9h-4V3H9v6H5l7 7 7-7z"></path></svg>
      </button>
    <%}%>
    
    <button disabled hx-get="/api/light-mode-dark-mode" hx-swap="innerHTML" hx-target="#application-styles" type="button" class="icon medium" aria-label="Light Mode / Dark Mode Toggle" id="light-mode-dark-mode">
      <svg focusable="false" inert viewBox="0 0 24 24">
        <path d="M20 8.69V4h-4.69L12 .69 8.69 4H4v4.69L.69 12 4 15.31V20h4.69L12 23.31 15.31 20H20v-4.69L23.31 12 20 8.69zM12 18c-.89 0-1.74-.2-2.5-.55C11.56 16.5 13 14.42 13 12s-1.44-4.5-3.5-5.45C10.26 6.2 11.11 6 12 6c3.31 0 6 2.69 6 6s-2.69 6-6 6z">
        </path>
      </svg>
    </button>
    <button class="icon medium" type="button" aria-label="Search" aria-haspopup="dialog" data-dialog="search-dialog">
      <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Search"><path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"></path></svg>
    </button>
    <button id="open-settings-dialog-btn" disabled type="button" aria-label="Edit Settings" aria-haspopup="dialog" data-dialog="settings-dialog" class="icon medium">
      <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Settings"><path d="M19.14 12.94c.04-.3.06-.61.06-.94 0-.32-.02-.64-.07-.94l2.03-1.58c.18-.14.23-.41.12-.61l-1.92-3.32c-.12-.22-.37-.29-.59-.22l-2.39.96c-.5-.38-1.03-.7-1.62-.94l-.36-2.54c-.04-.24-.24-.41-.48-.41h-3.84c-.24 0-.43.17-.47.41l-.36 2.54c-.59.24-1.13.57-1.62.94l-2.39-.96c-.22-.08-.47 0-.59.22L2.74 8.87c-.12.21-.08.47.12.61l2.03 1.58c-.05.3-.09.63-.09.94s.02.64.07.94l-2.03 1.58c-.18.14-.23.41-.12.61l1.92 3.32c.12.22.37.29.59.22l2.39-.96c.5.38 1.03.7 1.62.94l.36 2.54c.05.24.24.41.48.41h3.84c.24 0 .44-.17.47-.41l.36-2.54c.59-.24 1.13-.56 1.62-.94l2.39.96c.22.08.47 0 .59-.22l1.92-3.32c.12-.22.07-.47-.12-.61l-2.01-1.58zM12 15.6c-1.98 0-3.6-1.62-3.6-3.6s1.62-3.6 3.6-3.6 3.6 1.62 3.6 3.6-1.62 3.6-3.6 3.6z"></path></svg>
    </button>
  </div>
  <div class="navbar-3">
    <button id="notification-button" type="button" aria-label="Notifications" aria-haspopup="true" data-drawer-button data-drawer-el="#notification-sidebar" class="icon medium">
      <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="MoreVert"><path d="M12 8c1.1 0 2-.9 2-2s-.9-2-2-2-2 .9-2 2 .9 2 2 2zm0 2c-1.1 0-2 .9-2 2s.9 2 2 2 2-.9 2-2-.9-2-2-2zm0 6c-1.1 0-2 .9-2 2s.9 2 2 2 2-.9 2-2-.9-2-2-2z"></path></svg>
    </button>
  </div>
</header>

<div class="floating-menu" id="download-spa-menu" style="max-width: 250px; flex-direction: column; align-items: center;" data-placement="bottom-end" role="dialog" data-click-capture>
  <p class="body1 fw-regular text-align-center">
    Download the frankmbrown.net application today!
  </p>
  <div class="flex-row justify-center mt-2">
    <button type="button" class="medium icon-text filled primary" id="download-application">
      <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Download"><path d="M5 20h14v-2H5v2zM19 9h-4V3H9v6H5l7 7 7-7z"></path></svg>
      Download
    </button>
  </div>
</div>