<%# 
PARTIAL ROUTE: pages/posts/diary.ejs
PARTIAL DESCRIPTION: Description...
LOCAL VARIABLES:
----------------------------------
- locals.fullPageRequest
- locals.pageRequest
- locals.pageObj
%>
<%if(locals.fullPageRequest||locals.pageRequest){%>
  
  <h1 class="page-title"><%=locals.pageObj.title%></h1>
  <p class="body1 fw-regular mt-2 p-md"><%=locals.pageObj.description%></p>
  <div class="flex-row justify-between align-center bb p-md" style="margin-bottom: 0.75rem; border-bottom-width: 3px;">
    <time  data-popover data-pelem="#date-created-tool" data-mouse data-date-format="MM/DD/YYYY" datetime="<%-locals.pageObj.dateStringCreated%>">
      <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="CalendarToday">
        <path d="M20 3h-1V1h-2v2H7V1H5v2H4c-1.1 0-2 .9-2 2v16c0 1.1.9 2 2 2h16c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2zm0 18H4V8h16v13z">
        </path>
      </svg>
      <span></span>
    </time>
    <div class="flex-row gap-3 align-center">
      <a hx-target="#PAGE" hx-push-url="true" href="<%=locals.pageObj.editDiaryHref%>" hx-get="<%=locals.pageObj.editDiaryHref%>" hx-indicator="#page-transition-progress" class="button icon medium" aria-label="Edit Diary"><svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="EditNote"><path d="M3 10h11v2H3v-2zm0-2h11V6H3v2zm0 8h7v-2H3v2zm15.01-3.13.71-.71c.39-.39 1.02-.39 1.41 0l.71.71c.39.39.39 1.02 0 1.41l-.71.71-2.12-2.12zm-.71.71-5.3 5.3V21h2.12l5.3-5.3-2.12-2.12z"></path></svg></a>

      <time  data-popover data-pelem="#last-edit-tool" data-mouse data-date-format="MM/DD/YYYY" datetime="<%-locals.pageObj.dateStringEdited%>">
        <svg focusable="false" inert viewBox="0 0 24 24" tabindex="-1" title="Create"><path d="M3 17.25V21h3.75L17.81 9.94l-3.75-3.75L3 17.25zM20.71 7.04c.39-.39.39-1.02 0-1.41l-2.34-2.34a.9959.9959 0 0 0-1.41 0l-1.83 1.83 3.75 3.75 1.83-1.83z"></path></svg>
        <span></span>
      </time>
    </div>

  </div>
  

<%-locals.pageObj.innerHTML%>


<%-include('../../partials/pagePartial')%>
<%}%>